/*
1
23
345
4567
56789
67891011
*/











#include<stdio.h>
 
void main()
{
    int i,j,k,n;
 
    printf("Enter the no of lines\n");
    scanf("%d",&n);
 
    for(i=1;i<=n;i++)
    {
        k=i;
        for(j=1;j<=i;j++,k++)
        {
            printf("%d",k);
        }
         printf("\n");
    }
}